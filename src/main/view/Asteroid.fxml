<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>

<GridPane fx:controller="main.controller.RandomEventController"
          stylesheets="@Asteroid.css"
          xmlns:fx="http://javafx.com/fxml"
          alignment="center"
          hgap="10"
          vgap="10"
        >
    <Text fx:id="messageAsteroid"
          text="You have been hit by asteroids, your ship has been damaged!"
          GridPane.columnIndex="6"
          GridPane.rowIndex="6"
          stroke="white"
            />
    <Button fx:id="moveOn"
            text="OK!"
            GridPane.columnIndex="7"
            GridPane.rowIndex="6"
            onAction="#okRandEvent"
            />
</GridPane>